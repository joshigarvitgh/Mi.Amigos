<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="trial.trial_first.TemporaryPage">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/btn_geofencing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="GeoFencing --> FindPerson"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_assessment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Assessment"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_congratulations"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Congratulations"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_transparentOne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="DeviceId (Trans-1)"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_transparentTwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="LoginPassword (Trans-2)"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_transparentThree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Suggestions (Trans-3)"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_settings"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Settings"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_questionAndAnswer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Question and Answer"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_fnavigation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="FNavigation"
                android:textSize="25dp"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/btn_msg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Message"
                android:textSize="25dp"
                android:textAlignment="center"
                />

        </LinearLayout>

    </ScrollView>

</LinearLayout>
